<% @iptables_hosts.each do |host| %>
iptables -A INPUT -i eth0 -p tcp --dport 4567 -j ACCEPT -s <%= host %>
iptables -A INPUT -i eth0 -p tcp --dport 5672 -j ACCEPT -s <%= host %>
iptables -A INPUT -i eth0 -p tcp --dport 6379 -j ACCEPT -s <%= host %>
iptables -A INPUT -i eth0 -p tcp --dport 8080 -j ACCEPT -s <%= host %>
<% end %>
